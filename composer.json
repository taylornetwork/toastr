 {
    "name": "taylornetwork/toastr",
    "description": "A laravel class that will generate Toastr messages using toastr.js",
    "authors": [
        {
            "name": "Sam Taylor",
            "email": "sam@taylornetwork.ca"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "TaylorNetwork\\Toastr\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TaylorNetwork\\Toastr\\ToastrServiceProvider"
            ],
            "aliases": {
                "Toastr": "TaylorNetwork\\Toastr\\Facades\\Toastr"
            }
        }
    }
}
